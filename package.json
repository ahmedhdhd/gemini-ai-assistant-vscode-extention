{
  "name": "gemini-ai-assistant",
  "displayName": "Gemini AI Assistant",
  "description": "AI assistant powered by Google Gemini with custom context support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onView:geminiChat",
    "onView:geminiContext",
    "onView:geminiAgents"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "geminiAssistant.openChat",
        "title": "Open Gemini Assistant",
        "category": "Gemini",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "geminiAssistant.addContext",
        "title": "Add Custom Context",
        "category": "Gemini",
        "icon": "$(add)"
      },
      {
        "command": "geminiAssistant.clearContext",
        "title": "Clear Context",
        "category": "Gemini",
        "icon": "$(clear-all)"
      },
      {
        "command": "geminiAssistant.removeContext",
        "title": "Remove Context Item",
        "category": "Gemini",
        "icon": "$(close)"
      },
      {
        "command": "geminiAssistant.acceptSuggestion",
        "title": "Accept Suggestion",
        "category": "Gemini",
        "icon": "$(check)"
      },
      {
        "command": "geminiAssistant.declineSuggestion",
        "title": "Decline Suggestion",
        "category": "Gemini",
        "icon": "$(close)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "geminiAssistant",
          "title": "Gemini Assistant",
          "icon": "$(robot)"
        }
      ]
    },
    "views": {
      "geminiAssistant": [
        {
          "id": "geminiChat",
          "name": "AI Chat",
          "type": "webview"
        },
        {
          "id": "geminiContext",
          "name": "Custom Context"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "geminiAssistant.addContext",
          "when": "view == geminiContext",
          "group": "navigation"
        },
        {
          "command": "geminiAssistant.clearContext",
          "when": "view == geminiContext",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Gemini Assistant",
      "properties": {
        "geminiAssistant.aiProvider": {
          "type": "string",
          "default": "gemini",
          "description": "AI provider to use",
          "enum": [
            "gemini",
            "groq"
          ],
          "enumDescriptions": [
            "Google Gemini (requires Gemini API key)",
            "Groq (requires Groq API key)"
          ],
          "scope": "application"
        },
        "geminiAssistant.geminiApiKey": {
          "type": "string",
          "default": "",
          "description": "Your Google Gemini API key (get it from https://makersuite.google.com/app/apikey)",
          "scope": "application"
        },
        "geminiAssistant.geminiModel": {
          "type": "string",
          "default": "gemini-2.0-flash-exp",
          "description": "Gemini model to use",
          "enum": [
            "gemini-pro",
            "gemini-pro-vision",
            "gemini-2.0-flash-exp",
            "gemini-1.5-pro",
            "gemini-1.5-flash"
          ],
          "scope": "application"
        },
        "geminiAssistant.groqApiKey": {
          "type": "string",
          "default": "",
          "description": "Your Groq API key (get it from https://console.groq.com/keys)",
          "scope": "application"
        },
        "geminiAssistant.groqModel": {
          "type": "string",
          "default": "llama-3.3-70b-versatile",
          "description": "Groq model to use",
          "enum": [
            "llama-3.3-70b-versatile",
            "llama-3.1-70b-versatile",
            "llama-3.1-8b-instant",
            "mixtral-8x7b-32768",
            "gemma2-9b-it"
          ],
          "enumDescriptions": [
            "Llama 3.3 70B - Best overall performance",
            "Llama 3.1 70B - Previous generation",
            "Llama 3.1 8B - Fast and efficient",
            "Mixtral 8x7B - Good balance",
            "Gemma 2 9B - Lightweight model"
          ],
          "scope": "application"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}